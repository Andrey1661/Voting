@model  Voting.ViewModels.ListResponce<Voting.ViewModels.UserResponceViewModel>

<div class="row">
    
    <div class="col-md-6 col-md-offset-3">
        <div class="row">
            <table class="table">
                <tr>
                    <td>Фамилия</td>
                    <td>Имя</td>
                    <td>Отчество</td>
                    <td>Дата прохождения</td>
                    <td>Результат</td>
                </tr>

                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.LastName</td>
                        <td>@user.FirstName</td>
                        <td>@user.Patronymic</td>
                        <td>@user.PassDate</td>
                        <td>@user.Points</td>
                    </tr>
                }
            </table>
            
            @Html.ActionLink("Вернуться к анкете", "Questionnaire", "Home")
        </div>
    </div>

</div>

